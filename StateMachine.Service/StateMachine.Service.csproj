    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>dotnet-StateMachine.Service-8df4495b-81b8-4770-a2c3-e7d0a4b6679a</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MassTransit.EntityFrameworkCore" Version="8.1.2" />
